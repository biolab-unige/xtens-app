<h2><%=  params.groupId ?  __("group") + ": " + group.get("name") :
    params.dataTypeId ? __("data-type") + ": " + privileges.models[0].get("dataType").name : ""%></h2>

<div id="content">
  <div class="row">
      <div class="col-sm-12">
          <div class="table-responsive">
              <table class="table">
                  <thead>
                      <tr>
                          <th><%= __("group") %></th>
                          <th><%= __("data-type-name") %></th>
                          <th><%= __("privilege-level") %></th>
                          <th><%= __("project") %></th>
                          <th><%= __("actions") %></th>
                      </tr>
                  </thead>
                  <tbody>
                  <% _.each(privileges.models, function(privilege) { %>
                  <tr class="content">
                      <td><%= privilege.get("group").name %></td>
                      <td><%= privilege.get("dataType").name %></td>
                      <td><%= privilege.get("privilegeLevel").toUpperCase().replace('_', ' ') %></td>
                      <td><%= mapTypeProjects[privilege.get("dataType").id] ? mapTypeProjects[privilege.get("dataType").id] : "" %></td>

                      <td><a class="btn" href="#/datatypeprivileges/edit/<%= privilege.id %>"><%=__("edit") %></a></td>
                  </tr>
                  <%}) %>
                  </tbody>
              </table>
          </div>
          <div id="buttonbardiv" class="row text-center">
            <a href="" id="newPrivilege" class="btn btn-primary"><%= __("new-data-type-privilege")%></a>
          </div>
      </div>
  </div>
</div>
