{
  "name": "xtens-app",
  "version": "0.0.0",
  "description": "a digital repository for heterogeneous biomedical data",
  "main": "app.js",
  "keywords": [
    "data",
    "metadata",
    "data model"
  ],
  "authors": [
    "Massimiliano Izzo"
  ],
  "license": "BSD 3-clause",
  "homepage": "www.xtens.org",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "1.11.1",
    "bootstrap": "3.2.0",
    "underscore": null,
    "backbone": null,
    "font-awesome": "~4.2.0",
    "select2": "https://github.com/ivaynberg/select2.git#~3.5.1",
    "select2-bootstrap-css": "https://github.com/t0m/select2-bootstrap-css.git#~1.4.1",
    "backbone.stickit": "~0.8.0",
    "parsleyjs": "~2.0.5",
    "moment": "~2.8.3",
    "lodash": "~2.4.1",
    "d3": "~3.4.13",
    "datatables": "~1.10.4",
    "dropzone": "~4.0.1",
    "pikaday": "https://github.com/dbushell/Pikaday.git#~1.2.0"
  },
  "exportsOverride": {
    "jquery": {
      "js": "dist/jquery.js"
    },
    "bootstrap": {
      "less": "less",
      "js": "js/**/*.js"
    },
    "underscore": {
      "js": "**/underscore.js"
    },
    "lodash": {
      "js": "dist/lodash.js"
    },
    "backbone": {
      "js": "**/backbone.js"
    },
    "font-awesome": {
      "styles": "css/font-awesome.css",
      "fonts": "fonts/*.*"
    },
    "select2-bootstrap": {
      "": "**/select2-bootstrap.css"
    },
    "backbone.stickit": {
      "": "backbone.stickit.js"
    },
    "parsleyjs": {
     "js": "dist/parsley.js"
    },
    "d3": {
      "": "d3.js"
    },
    "sinon": {
      "js": "index.js"
    },
    "datatables": {
      "js": "media/js/jquery.dataTables.js",
      "styles": "media/css/jquery.dataTables.css",
      "images": "media/images/*"
    },
    "dropzone": {
      "js": "dist/*.js",
      "styles": "dist/*.css",
      "images": "downloads/images/*"
    },
    "pikaday": {
       "js": "pikaday.js",
       "styles": "css/pikaday.css" 
    }
  },
  "devDependencies": {
    "chai": "~1.9.2",
    "sinon-chai": "~2.5.0",
    "chai-jquery": "~1.2.2",
    "lodash": "~2.4.1",
    "sinon": "http://sinonjs.org/releases/sinon-1.12.1.js"
  }
}
