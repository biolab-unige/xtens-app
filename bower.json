{
  "name": "xtens-app",
  "version": "0.0.0",
  "description": "a digital repository for heterogeneous biomedical data",
  "main": "app.js",
  "keywords": [
    "data",
    "metadata",
    "data model"
  ],
  "authors": [
    "Massimiliano Izzo"
  ],
  "license": "BSD 3-clause",
  "homepage": "www.xtens.org",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.2",
    "backbone.stickit": "~0.9.2",
    "bootstrap": "~3.3.5",
    "bootstrap-select": "^1.12.2",
    "d3": "~3.5.17",
    "d3-tip":"~0.6.8",
    "datatables": "https://github.com/gicaz/DataTables.git#1.10.14-dev",
    "datatables.net": "https://github.com/gicaz/Dist-DataTables.git#1.10.13",
    "datatables-buttons": "~1.0.3",
    "datatables-plugins": "~1.10.7",
    "datatables.net-fixedcolumns": "^3.2.2",
    "dropzone": "~4.0.1",
    "font-awesome": "~4.2.0",
    "jquery": "~3.1.0",
    "jszip": "Stuk/jszip#~2.5.0",
    "lodash": "~3.10.0",
    "moment": "~2.8.3",
    "parsleyjs": "~2.0.5",
    "pikaday": "https://github.com/dbushell/Pikaday.git#~1.2.0",
    "select2": "https://github.com/ivaynberg/select2.git#~3.5.3",
    "select2-bootstrap-css": "https://github.com/t0m/select2-bootstrap-css.git#~1.4.1",
    "Sortable": "sortablejs#^1.5.1"
  },
  "exportsOverride": {
    "jquery": {
      "js": "dist/jquery.js"
    },
    "bootstrap": {
      "less": "less",
      "js": "dist/js/bootstrap.js"
    },
    "bootstrap-select": {
      "css": "dist/css/bootstrap-select.css",
      "js": "dist/js/bootstrap-select.js"
    },
    "underscore": {
      "js": "**/underscore.js"
    },
    "lodash": {
      "js": "lodash.js"
    },
    "backbone": {
      "js": "**/backbone.js"
    },
    "font-awesome": {
      "styles": "css/font-awesome.css",
      "fonts": "fonts/*.*"
    },
    "select2-bootstrap": {
      "": "**/select2-bootstrap.css"
    },
    "backbone.stickit": {
      "": "backbone.stickit.js"
    },
    "parsleyjs": {
      "js": "dist/parsley.js"
    },
    "d3": {
      "": "d3.js"
    },
    "sinon": {
      "js": "index.js"
    },
    "datatables": {
      "js": "media/js/jquery.dataTables.js",
      "images": "media/images/*"
    },
    "datatables-plugins": {
      "integration-bootstrap/js": "integration/bootstrap/3/dataTables.bootstrap.js",
      "integration-bootstrap/styles": "integration/bootstrap/3/dataTables.bootstrap.css",
      "integration-fontAwesome/styles": "integration/font-awesome/dataTables.fontAwesome.css"
    },
    "datatables-buttons": {
      "js": "js/*.js",
      "sass": "css/*.scss"
    },
    "jszip": {
      "js": "dist/jszip.js"
    },
    "dropzone": {
      "js": "dist/*.js",
      "styles": "dist/*.css",
      "images": "downloads/images/*"
    },
    "moment": {
      "js": "**/moment-with-locales.js"
    },
    "pikaday": {
      "js": "pikaday.js",
      "styles": "css/pikaday.css"
    }
  },
  "devDependencies": {
    "chai": "~1.9.2",
    "sinon-chai": "~2.5.0",
    "chai-jquery": "~1.2.2",
    "lodash": "~2.4.1",
    "sinon": "http://sinonjs.org/releases/sinon-1.12.1.js"
  }
}
